import subprocess

# Port range variable
port_range = "1-1024"

# Read IPs from file
with open('ip.txt', 'r') as file:
    lines = file.readlines()

# Loop through each line
for line in lines:
    parts = line.strip().split()
    if len(parts) >= 2:
        ip = parts[1]
        # Run nmap scan
        command = f"nmap -p {port_range} {ip}"
        subprocess.run(command, shell=True)
